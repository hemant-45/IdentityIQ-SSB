<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE Bundle PUBLIC "sailpoint.dtd" "sailpoint.dtd">
<Bundle displayName="User IT" name="User IT" type="it">
  <Attributes>
    <Map>
      <entry key="accountSelectorRules"/>
      <entry key="allowDuplicateAccounts" value="false"/>
      <entry key="allowMultipleAssignments" value="false"/>
      <entry key="mergeTemplates" value="false"/>
      <entry key="sysDescriptions">
        <value>
          <Map>
            <entry key="en_US"/>
          </Map>
        </value>
      </entry>
    </Map>
  </Attributes>
  <Inheritance>
    <Reference class="sailpoint.object.Bundle" name="IT Role"/>
  </Inheritance>
  <Owner>
    <Reference class="sailpoint.object.Identity" name="spadmin"/>
  </Owner>
  <Profiles>
    <Profile>
      <ApplicationRef>
        <Reference class="sailpoint.object.Application" name="Contractor System Employee"/>
      </ApplicationRef>
      <Constraints>
        <Filter operation="CONTAINS_ALL" property="Group">
          <Value>
            <List>
              <String>User</String>
            </List>
          </Value>
        </Filter>
      </Constraints>
    </Profile>
    <Profile>
      <ApplicationRef>
        <Reference class="sailpoint.object.Application" name="HR System"/>
      </ApplicationRef>
      <Constraints>
        <Filter operation="EQ" property="roles" value="User"/>
      </Constraints>
    </Profile>
  </Profiles>
</Bundle>